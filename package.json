    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "browserslist": [
